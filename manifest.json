{
  "manifest_version": 3,
  "name": "Altinn developer extension",
  "description": "Redirects all requests to a local Altinn app frontend",
  "version": "0.0.1",
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "ruleset",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://*/*"
  ]
}
